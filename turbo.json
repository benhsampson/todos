{
  "baseBranch": "origin/main",
  "pipeline": {
    "lint": {},
    "build": {
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "deploy": {
      "dependsOn": ["build"]
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    }
  },
  "globalDependencies": ["$GITHUB_TOKEN", ".env", "tsconfig.base.json"]
}
