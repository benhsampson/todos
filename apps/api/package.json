{
  "name": "@todos/api",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src",
    "build": "tsc",
    "build:watch": "yarn build -w --preserveWatchOutput >> /dev/null",
    "start": "node dist",
    "start:watch": "nodemon -w dist -e \"js\" dist -q -C",
    "dev": "run-p build:watch start:watch",
    "deploy": "echo \"DEPLOYING!\" && yarn start",
    "start2": "ts-node src"
  },
  "devDependencies": {
    "@types/node": "^17.0.27",
    "esbuild": "^0.14.38",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@todos/prisma": "0.0.0",
    "apollo-server": "^3.6.7",
    "dotenv": "^16.0.0",
    "graphql": "^16.4.0",
    "graphql-scalars": "^1.17.0",
    "nexus": "^1.3.0"
  }
}
