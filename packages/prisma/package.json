{
  "name": "@todos/prisma",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist",
  "types": "dist",
  "scripts": {
    "gen": "prisma generate --schema src/schema.prisma",
    "build": "tsc",
    "build:watch": "yarn build -w --preserveWatchOutput >> /dev/null",
    "start": "node dist",
    "start:watch": "nodemon -w dist -e \"js\" dist -q -C",
    "dev": "run-p build:watch start:watch"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.13.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0"
  }
}
